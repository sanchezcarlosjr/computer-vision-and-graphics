    "type": "code",
    "data": {
      "code": "interval(1000).pipe(\n  startWith(0),\n  concatMap(_ => fromFetch(`https://distributed-systems-movies.sanchezcarlosjr.repl.co/movies/${currentUrl.searchParams.get(\"imdbid\")}/global`, {\n  headers: {\n    \"Authorization\": \"Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJzdWIiOiIxMjM0NTY3ODkwIiwibmFtZSI6IkpvaG4gRG9lIiwiaWF0IjoxNTE2MjM5MDIyfQ.S2ZL7D-D3VeduQ44Cy2qLRFxHV43gRGSZtlfJ2MJ57g\"\n  }\n})),\n   map(x => ({\n      title: x.title,\n      cover: `<img src=\"${x.imageurl[0]}\">`,\n      genre: x.genre,\n      synopsis: x.synopsis,\n      global_mean: x.global_mean.toFixed(2),\n      local_rating: x.local_rating.toFixed(2),\n      local_users:  x.local_users\n   })),\n   jsonToTree(({key, valueIsObject, value, level}) => valueIsObject ? `<h${level}>${key}</h${level}>` : `<h2>${_.capitalize(key.replace(\"_\", \" \"))}</h2><p style=\"margin:0\">${value}</p>`)\n)",
